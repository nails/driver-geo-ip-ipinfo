{
    "name": "nails/driver-geo-ip-ipinfo",
    "keywords": ["Nails", "Shed Collective", "CodeIgniter", "GeoIP Module", "Driver", "IP Info"],
    "homepage": "http://nailsapp.co.uk/modules/geo-ip/driver/ipinfo",
    "description": "This is the \"IP Info\" driver for the Nails GeoIP module.",
    "license": "MIT",
    "authors":
    [
        {
            "name": "Pablo de la Pena",
            "email": "p@nailsapp.co.uk",
            "homepage": "http://nailsapp.co.uk",
            "role": "Developer"
        }
    ],
    "support":
    {
        "email": "hello@nailsapp.co.uk"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require":
    {
        "nails/module-geo-ip": "dev-develop"
    },
    "require-dev":
    {
        "phpunit/phpunit": "7.5.1"
    },
    "extra":
    {
        "nails" :
        {
            "name": "IP Info",
            "type": "driver",
            "forModule": "nails/module-geo-ip",
            "data": {
                "namespace": "Nails\\GeoIp\\Driver\\",
                "class": "IpInfo",
                "settings": [
                    {
                        "key": "sAccessToken",
                        "label": "Access Token",
                        "type": "password",
                        "encrypted": true
                    }
                ]
            }
        }
    },
    "autoload":
    {
        "psr-4": {"Nails\\GeoIp\\Driver\\": "src/"}
    }
}
